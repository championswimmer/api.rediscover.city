    "@elysiajs/swagger": "^1.3.1",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@sinclair/typebox": "^0.34.38",
    "@sinclair/typebox-codegen": "^0.11.1",
    "@types/ngeohash": "^0.6.8",
    "@typeschema/typebox": "^0.14.0",
    "adze": "^2.2.5",
    "ai": "^5.0.15",
